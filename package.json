{
  "name": "javascript-tests",
  "version": "1.0.0",
  "description": "A comparison of test frameworks in javascript",
  "main": "public/index.mjs",
  "scripts": {
    "pretest": "standard",
    "test": "npm run test-tape && npm run test-mocha",
    "test-tape": "babel-tape-runner test/test.tape.js | faucet",
    "test-mocha": "mocha --require babel-register test/test.mocha.js",
    "test-jest": "jest --testRegex /__tests__/.*\\.jest\\.js$",
    "start": "parcel src/index.html",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terabaud/javascript-tests.git"
  },
  "keywords": [],
  "author": "Lea Rosema",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/terabaud/javascript-tests/issues"
  },
  "homepage": "https://github.com/terabaud/javascript-tests#readme",
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-tape-runner": "^2.0.1",
    "chai": "^4.1.2",
    "faucet": "0.0.1",
    "gh-pages": "^1.2.0",
    "jest": "^23.2.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.9.3",
    "regenerator-runtime": "^0.12.0",
    "standard": "^11.0.1",
    "tape": "^4.9.1"
  }
}
